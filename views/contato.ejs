<%- include("header.ejs")%>
  <%- include('nav.ejs') %>

    <section class="container">
      <article class="col-sm-8">
        <form action="" method="POST" class="mt-5 mb-5">
          <div class="form-group">
            <label for="cad_nome">Nome</label>
            <input type="text" class="form-control" id="cad_nome" placeholder="Seu nome aqui" name="cad_nome">
          </div>
          <div class="form-group">
            <label for="cad_sobrenome">Sobrenome</label>
            <input type="text" class="form-control" id="cad_sobrenome" placeholder="Seu sobrenome aqui"
              name="cad_sobrenome">
          </div>
          <div class="form-group">
            <label for="cad_email">Email</label>
            <input type="email" class="form-control" id="cad_email" name="cad_email"
              placeholder="Seu email aqui">
          </div>
          <div class="form-group">
            <label for="cad_mensagem">Mensagem</label>
            <textarea name="cad_mensagem" class="form-control"></textarea>
          </div>
          <button type="submit" class="btn btn-primary">Confirmar</button>
        </form>
      </article>
    </section>

    <script src="../js/contato.js"></script>

    <%- include("footer.ejs")%>